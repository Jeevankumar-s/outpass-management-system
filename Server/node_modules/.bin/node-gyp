#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/bin/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/node-gyp@8.4.1/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules:/home/workspace/reactjs/sessions/sorting-products/node_modules:/home/workspace/reactjs/sessions/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/node-gyp/bin/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/node-gyp/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/bin/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/node-gyp@8.4.1/node_modules/node-gyp/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/node-gyp@8.4.1/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules:/home/workspace/reactjs/sessions/sorting-products/node_modules:/home/workspace/reactjs/sessions/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/node-gyp/bin/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/node-gyp/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-gyp/bin/node-gyp.js" "$@"
else
  exec node  "$basedir/../node-gyp/bin/node-gyp.js" "$@"
fi
