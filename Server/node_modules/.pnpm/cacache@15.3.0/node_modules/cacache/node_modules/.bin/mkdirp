#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/mkdirp@1.0.4/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules:/home/workspace/reactjs/sessions/sorting-products/node_modules:/home/workspace/reactjs/sessions/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/cacache@15.3.0/node_modules/cacache/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/cacache@15.3.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/mkdirp@1.0.4/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules:/home/workspace/reactjs/sessions/sorting-products/node_modules:/home/workspace/reactjs/sessions/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/cacache@15.3.0/node_modules/cacache/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/cacache@15.3.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
fi
