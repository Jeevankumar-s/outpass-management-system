#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/@mapbox+node-pre-gyp@1.0.11/node_modules/@mapbox/node-pre-gyp/bin/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/@mapbox+node-pre-gyp@1.0.11/node_modules/@mapbox/node-pre-gyp/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/@mapbox+node-pre-gyp@1.0.11/node_modules/@mapbox/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/@mapbox+node-pre-gyp@1.0.11/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules:/home/workspace/reactjs/sessions/sorting-products/node_modules:/home/workspace/reactjs/sessions/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/@mapbox+node-pre-gyp@1.0.11/node_modules/@mapbox/node-pre-gyp/bin/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/@mapbox+node-pre-gyp@1.0.11/node_modules/@mapbox/node-pre-gyp/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/@mapbox+node-pre-gyp@1.0.11/node_modules/@mapbox/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/@mapbox+node-pre-gyp@1.0.11/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules/.pnpm/node_modules:/home/workspace/reactjs/sessions/sorting-products/Server/node_modules:/home/workspace/reactjs/sessions/sorting-products/node_modules:/home/workspace/reactjs/sessions/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/node-pre-gyp" "$@"
else
  exec node  "$basedir/../../bin/node-pre-gyp" "$@"
fi
